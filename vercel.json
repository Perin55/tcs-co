{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "pages/api/**/*",
        "use": "@vercel/node"
      },
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      },
      {
        "src": "/.*",
        "use": "@vercel/edge-functions"
      }
    ],
